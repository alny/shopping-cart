<div class="row">
{{#each products}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{imagePath}}" alt="...">
      <div class="caption">
        <h3>{{title}}</h3>
        <p>{{description}}</p>
        <p><a href="#" class="btn btn-primary" role="button">Add to Cart</a> <a href="#" class="btn btn-default" role="button">$ {{price}}</a></p>
      </div>
    </div>
    </div>
  {{/each}}
</div>
